main {
	& > ul {
		list-style: none;
		padding: 0;
		& > li {
			margin: 2rem;
			padding: 1rem;
			& > a {
				display: flex;
				align-items: center;
				justify-content: space-between;
				width: 100%;
				header {
					margin-bottom: 0;
					h2 {
						margin: 0;
					}
				}
				font-weight: normal;
				font-family: 'Fira Code', monospace;
				& > img {
					margin: 0;
					margin-inline-start: 1em;
				}
			}
		}
	}
}
