{
	"name": "abogical",
	"private": true,
	"version": "0.1.0",
	"description": "Abdelrahman Abdelrahman's personal site.",
	"scripts": {
		"clean": "rimraf _site",
		"cleanBuild": "run-s clean build",
		"build": "mkdirp ./_site/assets ./.cache && cross-env NODE_ENV='production' run-s build:eleventy build:postcss build:rollup build:posthtml",
		"build:eleventy": "eleventy",
		"build:postcss": "postcss ./src/assets/*.pcss -d ./_site --ext '.css' --no-map",
		"build:rollup": "rollup -c rollup.config.ts --configPlugin typescript",
		"build:posthtml": "posthtml -a",
		"dev": "mkdirp ./_site/assets ./.cache && cross-env NODE_ENV='development' run-p dev:*",
		"dev:eleventy": "eleventy --serve",
		"dev:postcss": "postcss ./src/assets/*.pcss -d ./_site --ext '.css' -w",
		"dev:rollup": "rollup -c rollup.config.ts --configPlugin typescript -w",
		"serve": "sirv _site",
		"debug": "cross-env DEBUG=* npm run dev",
		"lint": "xo && prettier --check --ignore-path .gitignore **/*.json",
		"lintFix": "xo --fix && prettier --write --ignore-path .gitignore **/*.json"
	},
	"repository": {
		"type": "git",
		"url": "https://gitlab.com/Abogical/abogical.git"
	},
	"author": {
		"name": "Abdelrahman Abdelrahman",
		"email": "abogical@gmail.com",
		"url": "https://abogic.al"
	},
	"license": "AGPL-3.0-or-later",
	"homepage": "https://abogic.al",
	"devDependencies": {
		"@rollup/plugin-typescript": "^8.3.0",
		"rimraf": "^3.0.2",
		"sirv-cli": "^2.0.1",
		"xo": "^0.48.0"
	},
	"dependencies": {
		"@11ty/eleventy": "^1.0.1",
		"@11ty/eleventy-plugin-rss": "^1.1.2",
		"@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
		"@babel/core": "^7.16.7",
		"@babel/preset-env": "^7.16.8",
		"@fontsource/fira-code": "^4.5.2",
		"@fontsource/roboto-slab": "^4.5.1",
		"@fullhuman/postcss-purgecss": "^4.1.3",
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-node-resolve": "^13.1.3",
		"cheerio": "^1.0.0-rc.10",
		"cross-env": "^7.0.3",
		"cssnano": "^5.0.15",
		"cssnano-preset-advanced": "^5.1.10",
		"date-fns": "^2.28.0",
		"eleventy-plugin-lazyimages": "^2.1.2",
		"eleventy-plugin-safe-external-links": "^0.1.5",
		"html-minifier-terser": "^6.1.0",
		"htmlnano": "^2.0.0",
		"lazysizes": "^5.3.2",
		"markdown-it": "^12.3.2",
		"markdown-it-anchor": "^8.4.1",
		"mkdirp": "^1.0.4",
		"npm-run-all": "^4.1.5",
		"postcss-cli": "^9.1.0",
		"postcss-import": "^14.0.2",
		"postcss-preset-env": "^7.2.3",
		"postcss-reporter": "^7.0.5",
		"postcss-url": "^10.1.3",
		"posthtml-cli": "^0.10.0",
		"posthtml-sri": "^1.2.0",
		"prismjs": "^1.26.0",
		"remixicon": "^2.5.0",
		"rollup": "^2.64.0",
		"rollup-plugin-multi-input": "^1.3.1",
		"rollup-plugin-terser": "^7.0.2",
		"svgo": "^2.8.0"
	}
}
