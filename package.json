{
	"name": "abogical",
	"private": true,
	"version": "0.1.0",
	"description": "Abdelrahman Abdelrahman's personal site.",
	"scripts": {
		"clean": "rimraf _site",
		"cleanBuild": "run-s clean build",
		"build": "mkdirp ./_site/assets ./.cache && cross-env NODE_ENV='production' run-s build:eleventy build:postcss build:rollup build:posthtml",
		"build:eleventy": "eleventy",
		"build:postcss": "postcss ./src/assets/*.pcss -d ./_site --ext '.css' --no-map",
		"build:rollup": "rollup -c rollup.config.ts --configPlugin typescript",
		"build:posthtml": "posthtml -a",
		"dev": "mkdirp ./_site/assets ./.cache && cross-env NODE_ENV='development' run-p dev:*",
		"dev:eleventy": "eleventy --serve",
		"dev:postcss": "postcss ./src/assets/*.pcss -d ./_site --ext '.css' -w",
		"dev:rollup": "rollup -c rollup.config.ts --configPlugin typescript -w",
		"serve": "sirv _site",
		"debug": "cross-env DEBUG=* npm run dev",
		"lint": "xo && prettier --check --ignore-path .gitignore **/*.json",
		"lintFix": "xo --fix && prettier --write --ignore-path .gitignore **/*.json"
	},
	"repository": {
		"type": "git",
		"url": "https://gitlab.com/Abogical/abogical.git"
	},
	"author": {
		"name": "Abdelrahman Abdelrahman",
		"email": "abogical@gmail.com",
		"url": "https://abogic.al"
	},
	"license": "AGPL-3.0-or-later",
	"homepage": "https://abogic.al",
	"devDependencies": {
		"@rollup/plugin-typescript": "^8.2.4",
		"rimraf": "^3.0.2",
		"sirv-cli": "^1.0.10",
		"xo": "^0.42.0"
	},
	"dependencies": {
		"@11ty/eleventy": "^0.12.1",
		"@11ty/eleventy-navigation": "^0.1.6",
		"@11ty/eleventy-plugin-rss": "^1.0.9",
		"@11ty/eleventy-plugin-syntaxhighlight": "^3.0.4",
		"@babel/core": "^7.12.10",
		"@babel/preset-env": "^7.12.11",
		"@fontsource/fira-code": "^4.5.0",
		"@fontsource/roboto-slab": "^4.5.0",
		"@fullhuman/postcss-purgecss": "^3.1.3",
		"@rollup/plugin-babel": "^5.2.2",
		"@rollup/plugin-node-resolve": "^9.0.0",
		"cheerio": "^1.0.0-rc.3",
		"cross-env": "^7.0.3",
		"cssnano": "^4.1.10",
		"cssnano-preset-advanced": "^4.0.7",
		"date-fns": "^2.16.1",
		"eleventy-plugin-lazyimages": "^2.1.0",
		"eleventy-plugin-safe-external-links": "^0.1.5",
		"html-minifier-terser": "^5.1.1",
		"htmlnano": "^1.0.0",
		"lazysizes": "^5.2.2",
		"markdown-it": "^8.4.2",
		"markdown-it-anchor": "^5.3.0",
		"mkdirp": "^1.0.4",
		"npm-run-all": "^4.1.5",
		"postcss-cli": "^7.1.2",
		"postcss-import": "^12.0.1",
		"postcss-preset-env": "^6.7.0",
		"postcss-reporter": "^6.0.1",
		"postcss-url": "^8.0.0",
		"posthtml-cli": "^0.8.1",
		"posthtml-sri": "^1.0.5",
		"prismjs": "^1.22.0",
		"remixicon": "^2.5.0",
		"rollup": "^2.35.1",
		"rollup-plugin-multi-input": "^1.1.1",
		"rollup-plugin-terser": "^7.0.2"
	}
}